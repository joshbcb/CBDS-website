---
import Layout from "../layouts/Layout.astro"
import Header from "../components/Header.astro"
import Footer from "../components/Footer.astro"
import ImageGallery from "../components/ImageGallery.astro"

/* Data */
import { galleries } from "../data/imageGalleries.js";
---

<Layout>
    <Header />
    <ImageGallery
        header="Photos"
        images={galleries["photos"].images}
     />
     <ImageGallery
        header="Wedding Photos"
        images={galleries["photos"].images}
     />
     <ImageGallery
        header="Fall Showcase 2025"
        images={galleries["photos"].images}
     />
     <ImageGallery
        header="Kings Ball 2025"
        images={galleries["photos"].images}
     />
    <Footer />
</Layout>